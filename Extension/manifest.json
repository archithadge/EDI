
{
    "name": "Parental Control",
    "version": "1.0",
    "description": "Kids u r gone",
    "permissions": ["storage","activeTab"],
    "background": {
        "scripts": ["background.js"],
        "persistent": false
      },
    "browser_action": {
        "default_popup": "popup.html"
      },
    "manifest_version": 2,
    "content_scripts": [
      {
        "matches": [
          "<all_urls>"
        ],
        "js": ["content.js"]
      }
    ]
  }